#!/usr/bin/env python2

import mpd 					                       
import os

client = mpd.MPDClient()
client.connect("localhost", 6600)					  #

curtitle = client.currentsong()['title']

pos = 0
count = 0

for x in client.playlistinfo():
    if x['title'] != curtitle and pos == 0:
	pass
    elif x['title'] == curtitle:
        pos = 1
    elif count < 5:
        print x['title']
	count +=1
    else:
	break

